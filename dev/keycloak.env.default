# Example docker-compose environment file
# Copy to keycloak.env and modify as necessary
# https://docs.docker.com/compose/env-file/

# Variables defined in this file will only be available to containers/images
# ie not for interpolation in docker-compose YAML files

# KEYCLOAK_EMAIL_USER=
# KEYCLOAK_EMAIL_PASSWORD=

# must match OAUTH2_PROXY_CLIENT_SECRET in logs/.env
# KEYCLOAK_WAHS_LOGS_CLIENT_SECRET=

# Login branding overrides
# KEYCLOAK_LOGIN_THEME_LOGO_FILENAME=
# KEYCLOAK_LOGIN_THEME_COMPANY_LOGO_FILENAME=
# KEYCLOAK_LOGIN_THEME_FAVICON_FILENAME=
# KEYCLOAK_REALM_NAME=

# For google idp
KEYCLOAK_GOOGLE_IDP_ENABLED=FALSE
KEYCLOAK_GOOGLE_IDP_CLIENT_ID=
KEYCLOAK_GOOGLE_IDP_CLIENT_SECRET=